#include <opencv2/opencv.hpp>
#include <stdio.h>;
#include <iostream>

int main( int argc, char** argv )
{
	std::cout << "test";
  Mat image;
  image = imread( argv[1], IMREAD_COLOR );
  if( argc != 2 || !image.data )
    {
      printf( "No image data \n" );
      return -1;
    }
  namedWindow( "Display Image", WINDOW_AUTOSIZE );
  imshow( "Display Image", image );
  waitKey(0);
  return 0;
}
